{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "main": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular": {
          "style": "scss",
          "styleExt": "scss",
          "skipTests": true,
          "changeDetection": "OnPush"
        }
      },
      "root": "projects/main",
      "sourceRoot": "projects/main/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/main",
            "index": "projects/main/src/index.html",
            "main": "projects/main/src/main.ts",
            "polyfills": "projects/main/src/polyfills.ts",
            "tsConfig": "projects/main/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/main/src/assets"
            ],
            "styles": [
              "projects/main/src/styles.scss",
              "theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/main/src/environments/environment.ts",
                  "with": "projects/main/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "main:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "main:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "main:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/main/src/test.ts",
            "polyfills": "projects/main/src/polyfills.ts",
            "tsConfig": "projects/main/tsconfig.spec.json",
            "karmaConfig": "projects/main/karma.conf.js",
            "assets": [
              "projects/main/src/assets"
            ],
            "styles": [
              "projects/main/src/styles.scss",
              "theme.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/main/tsconfig.app.json",
              "projects/main/tsconfig.spec.json",
              "projects/main/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/main/e2e/protractor.conf.js",
            "devServerTarget": "main:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main:serve:production"
            }
          }
        }
      }
    },
    "map-creator-object-choice": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular": {
          "style": "scss",
          "styleExt": "scss",
          "skipTests": true,
          "changeDetection": "OnPush"
        }
      },
      "root": "projects/map-creator-object-choice",
      "sourceRoot": "projects/map-creator-object-choice/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/map-creator-object-choice",
            "index": "projects/map-creator-object-choice/src/index.html",
            "main": "projects/map-creator-object-choice/src/main.ts",
            "polyfills": "projects/map-creator-object-choice/src/polyfills.ts",
            "tsConfig": "projects/map-creator-object-choice/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/map-creator-object-choice/src/assets"
            ],
            "styles": [
              "projects/map-creator-object-choice/src/styles.scss",
              "theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/map-creator-object-choice/src/environments/environment.ts",
                  "with": "projects/map-creator-object-choice/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "map-creator-object-choice:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "map-creator-object-choice:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "map-creator-object-choice:build"
          }
        }
      }
    },
    "rage-connector": {
      "projectType": "library",
      "root": "projects/rage-connector",
      "sourceRoot": "projects/rage-connector/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/rage-connector/tsconfig.lib.json",
            "project": "projects/rage-connector/ng-package.json"
          }
        ,          "configurations": {
            "production": {
              "tsConfig": "projects/rage-connector/tsconfig.lib.prod.json"
            }
          }
},
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rage-connector/src/test.ts",
            "tsConfig": "projects/rage-connector/tsconfig.spec.json",
            "karmaConfig": "projects/rage-connector/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/rage-connector/tsconfig.lib.json",
              "projects/rage-connector/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "map-creator-vehicle-choice": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular": {
          "style": "scss",
          "styleExt": "scss",
          "skipTests": true,
          "changeDetection": "OnPush"
        }
      },
      "root": "projects/map-creator-vehicle-choice",
      "sourceRoot": "projects/map-creator-vehicle-choice/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/map-creator-vehicle-choice",
            "index": "projects/map-creator-vehicle-choice/src/index.html",
            "main": "projects/map-creator-vehicle-choice/src/main.ts",
            "polyfills": "projects/map-creator-vehicle-choice/src/polyfills.ts",
            "tsConfig": "projects/map-creator-vehicle-choice/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/map-creator-vehicle-choice/src/assets"
            ],
            "styles": [
              "projects/map-creator-vehicle-choice/src/styles.scss",
              "theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/map-creator-vehicle-choice/src/environments/environment.ts",
                  "with": "projects/map-creator-vehicle-choice/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "map-creator-vehicle-choice:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "map-creator-vehicle-choice:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "map-creator-vehicle-choice:build"
          }
        }
      }
    }
  },
  "defaultProject": "main"
}
